<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" id="myModalLabel"><span class="fs50">Najlepsze dostępne miejsca</span></h4>
</div>
<div class="modal-body reserveBody">
    <div class="ticketManage">
        <div class="row">
            <div class="col-md-12">
                @*<strong class="reservationFonts">Cena biletu:  PLN</strong>*@
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <strong class="reservationFonts">Liczba biletów</strong>
            </div>
            <div class="col-md-6">
                <select class="form-control ticketCount">
                    @for (int i = 1; i < 10; i++)
                    {
                        <option value="@i">@i</option>
                    }
                </select>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default closeRsrv" data-dismiss="modal">Anuluj</button>
    <button type="button" class="btn btn-primary reserveBtn">Rezerwuj</button>
</div>

